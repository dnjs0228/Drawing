function setup() {
  createCanvas(640, 480);
  //background(red, green, blue);
  background(0, 255, 0);
  fill(255, 0, 0);

   stroke(255,0,0)
   line(10, 10, 90, 10); // x1, y1, x2, y2

  	fill(200, 249, 103);
  	stroke(200, 249, 103)
    rect(20, 20, 80, 50); // x, y, width, height

    fill(9, 112, 103);
  	stroke(9, 112, 103)
    ellipse(50, 50, 50, 50); // centerX, centerY, width, height
}